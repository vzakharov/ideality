{
  "settings": {
    "cut": {
      "maxTokens": 1000,
      "anchor": "%CUT FROM HERE%\n",
      "replaceWith": "\n(...)\n"
    },
    "fetch": {
      "url": "https://api.openai.com/v1/engines/<Engine>/completions",
      "auth": "Bearer <API key>",
      "body": "{ prompt: \"<Prompt>\", temperature: <Temperature>, max_tokens: <Max tokens>, frequency_penalty: <Frequency penalty>, presence_penalty: <Presence penalty>, n: <Number of outputs> }"
    }
  },
  "parameters": [
    {
      "name": "API key",
      "type": "password"
    },
    {
      "name": "Engine",
      "type": "choice",
      "options": [
        "davinci",
        "curie",
        "babbage",
        "ada",
        "davinci-instruct-beta",
        "curie-instruct-beta"
      ],
      "text": "curie"
    },
    {
      "name": "Temperature",
      "type": "decimal",
      "min": 0,
      "max": 1,
      "number": 0.5
    },
    {
      "name": "Max tokens",
      "type": "integer",
      "min": 1,
      "number": 50
    },
    {
      "name": "Frequency penalty",
      "type": "decimal",
      "min": 0,
      "max": 1,
      "number": 1
    },
    {
      "name": "Presence penalty",
      "type": "decimal",
      "min": 0,
      "max": 1,
      "number": 1
    },
    {
      "name": "Number of outputs",
      "type": "integer",
      "min": 1,
      "number": 3
    }
  ]
}